{
  "hooks": {
    "preToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "cat | curl -s -X POST http://127.0.0.1:61670/hook/${CLUBHOUSE_AGENT_ID} -H 'Content-Type: application/json' -H \"X-Clubhouse-Nonce: ${CLUBHOUSE_HOOK_NONCE}\" --data-binary @- || true",
            "async": true,
            "timeout": 5
          }
        ]
      }
    ],
    "postToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "cat | curl -s -X POST http://127.0.0.1:61670/hook/${CLUBHOUSE_AGENT_ID} -H 'Content-Type: application/json' -H \"X-Clubhouse-Nonce: ${CLUBHOUSE_HOOK_NONCE}\" --data-binary @- || true",
            "async": true,
            "timeout": 5
          }
        ]
      }
    ],
    "errorOccurred": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "cat | curl -s -X POST http://127.0.0.1:61670/hook/${CLUBHOUSE_AGENT_ID} -H 'Content-Type: application/json' -H \"X-Clubhouse-Nonce: ${CLUBHOUSE_HOOK_NONCE}\" --data-binary @- || true",
            "async": true,
            "timeout": 5
          }
        ]
      }
    ],
    "sessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "cat | curl -s -X POST http://127.0.0.1:61670/hook/${CLUBHOUSE_AGENT_ID} -H 'Content-Type: application/json' -H \"X-Clubhouse-Nonce: ${CLUBHOUSE_HOOK_NONCE}\" --data-binary @- || true",
            "async": true,
            "timeout": 5
          }
        ]
      }
    ]
  }
}